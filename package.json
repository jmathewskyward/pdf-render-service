{
  "name": "pdfengine",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Nicholas Elliott",
  "license": "NONE",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "start": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "build": "tsc",
    "debug": "cross-env TZ=UTC NODE_ENV=development tsnd --inspect --respawn src/index.ts",
    "release": "tsc && node ./dist/index.js",
    "update-types": "npx ts-json-schema-generator -f tsconfig.json -i PdfRequest --path ./src/wire/PdfRequest.ts --type PdfRequest -o ./src/resources/PdfRequest.json",
    "profile": "tsc && node --prof ./dist/index.js"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.50.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.50.0",
    "@opentelemetry/instrumentation": "^0.50.0",
    "@opentelemetry/instrumentation-http": "^0.50.0",
    "@opentelemetry/resources": "^1.23.0",
    "@opentelemetry/sdk-trace-base": "^1.23.0",
    "@opentelemetry/sdk-trace-node": "^1.23.0",
    "@opentelemetry/semantic-conventions": "^1.23.0",
    "@react-pdf/renderer": "^3.0.0",
    "ajv": "^8.11.0",
    "axios": "^0.28.0",
    "cors": "^2.8.5",
    "fontkit": "2",
    "fonts": "^0.0.2",
    "prometheus-middleware": "^1.3.5",
    "react": "^16.12.0",
    "uuid": "^3.3.3",
    "vm2": "^3.9.18",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/fontkit": "^1.8.0",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.12",
    "@types/request": "^2.48.3",
    "@types/uuid": "^3.4.6",
    "cross-env": "^6.0.3",
    "ts-json-schema-generator": "^1.0.0",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.0.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4"
  },
  "volta": {
    "node": "18.17.0",
    "yarn": "3.6.1"
  },
  "packageManager": "yarn@3.6.1"
}
